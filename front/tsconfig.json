{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/redux/actions/user.js",
    "src/app/account/register/page.jsx",
    "src/lib/auth/authSlice.js",
    "src/app/account/login/page.jsx",
    "src/app/account/activate/page.jsx",
    "src/Components/PretectedRoute.jsx",
    "src/Components/Account/LogOut.jsx",
    "src/app/account/resetPassword/page.jsx",
    "src/app/account/resetPasswordConfirm/page.jsx",
    "src/app/account/resetPasswordConfirm/page.jsx",
    "src/app/account/resetPassword/page.jsx",
    "src/app/account/resetPasswordConfirm/page.jsx",
    "src/app/account/resetPassword/page.jsx",
    "src/app/account/page.jsx",
    "src/Components/Header/header.jsx",
    "src/app/page.jsx"
  ],
  "exclude": ["node_modules"]
}
